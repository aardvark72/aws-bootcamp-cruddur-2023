export DB_SG_ID="sg-0e227aae723891036"
gp env DB_SG_ID="sg-0e227aae723891036"

export DB_SG_RULE_ID="sgr-0ede9a050e45f969f"
gp env DB_SG_RULE_ID="sgr-0ede9a050e45f969f"

aws ec2 modify-security-group-rules \
    --group-id $DB_SG_ID \
    --security-group-rules "SecurityGroupRuleId=$DB_SG_RULE_ID,SecurityGroupRule={IpProtocol=tcp,FromPort=5432,ToPort=5432,CidrIpv4=$GITPOD_IP/32}"